{
  "presets": [
    "react-native"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": "./src",
        "alias": {
          "assets": "./assets",
          "lib": "./lib",
          "tests": "./tests"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console"
      ]
    }
  }
}
